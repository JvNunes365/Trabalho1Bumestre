package Trabalho;

public class Exercicio07 
        {
	
		public static void main(String[]args) 
		
		{
			String teste = ("  teste     123  ");
			System.out.println ("Imprima essa String: " + teste);
			System.out.println("Imprima essa String sem Espaços no Inicio e Final:" + teste.trim());
			String testeSemEspacos = teste.trim().replaceAll("\\s+", " ");
			System.out.println("Imprima essa String sem Espaços no Inicio e Final e com apenas 1 espaço no meio:" + testeSemEspacos);
		

	}

}
